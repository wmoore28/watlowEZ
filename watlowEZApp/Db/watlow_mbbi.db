record(mbbi, "$(P)$(R)AIN_ERR_DESC") {
    field(DTYP, "Raw Soft Channel")
    field(INP,  "$(P)$(R)AIN_ERR.VAL")
    field(ZRST, "None")
    field(ZRVL, "61")
    field(ONST, "Open")
    field(ONVL, "65")
    field(TWST, "Fail")
    field(TWVL, "32")
    field(THST, "Shorted")
    field(THVL, "127")
    field(FRST, "Measurement Err")
    field(FRVL, "140")
    field(FVST, "Bad Cal. Data")
    field(FVVL, "139")
    field(SXST, "Ambient Error")
    field(SXVL, "9")
    field(SVST, "RTD Error")
    field(SVVL, "141")
